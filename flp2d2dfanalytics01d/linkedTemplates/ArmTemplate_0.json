		{
			"name": "[concat(parameters('factoryName'), '/OracleDB_Dataset')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "OracleDB",
					"type": "LinkedServiceReference",
					"parameters": {
						"Host": {
