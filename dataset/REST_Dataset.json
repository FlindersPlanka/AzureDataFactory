{
	"name": "REST_Dataset",
	"properties": {
		"description": "Dataset for JSON response from REST API.",
		"linkedServiceName": {
			"referenceName": "RESTService",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"RelativeUrl": {
				"type": "string"
			},
			"PaginationJsonPath": {
				"type": "string"
			},
			"ApiKey": {
				"type": "string"
			},
			"Authorization": {
				"type": "string"
			}
		},
		"folder": {
			"name": "Final Datasets"
		},
		"annotations": [],
		"type": "RestResource",
		"typeProperties": {
			"relativeUrl": {
				"value": "@dataset().RelativeUrl",
				"type": "Expression"
			},
			"requestMethod": "GET",
			"additionalHeaders": {
				"Authorization": {
					"value": "@if(equals(dataset().Authorization, ''), '-1', dataset().Authorization)",
					"type": "Expression"
				},
				"api-key": {
					"value": "@dataset().ApiKey",
					"type": "Expression"
				}
			},
			"paginationRules": {
				"AbsoluteUrl": {
					"value": "@dataset().PaginationJsonPath",
					"type": "Expression"
				}
			}
		}
	},
	"type": "Microsoft.DataFactory/factories/datasets"
}